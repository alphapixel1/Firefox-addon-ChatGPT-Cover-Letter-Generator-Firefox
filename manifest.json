{
  "manifest_version":2,
  "name":"Helper",
  "version":"1.0.0",
  "content_scripts": [
    {
      "matches":["https://www.linkedin.com/*"],
      "js":["LinkedIn.js"],
      "run_at":"document_idle"
    }
  ],
  "browser_action":{
      "browser_style":true,
      "default_icon":{
          "96":"images/96x96.png"
      },
      "default_title":"Instructure Quizlet",
      "default_popup":"Popup/PopupV5.html"
  },
  "permissions":[
      "<all_urls>",
      "webNavigation",
      "tabs",
      "storage",
      "webRequest",
      "clipboardRead",
      "clipboardWrite"

  ]
}